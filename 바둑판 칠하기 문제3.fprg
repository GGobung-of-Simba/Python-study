    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="i" type="Integer" array="False" size=""/>
            <assign variable="i" expression="1"/>
            <while expression="i&lt;=10">
                <declare name="j" type="Integer" array="False" size=""/>
                <assign variable="j" expression="1"/>
                <declare name="b" type="String" array="False" size=""/>
                <assign variable="b" expression="&quot;&quot;"/>
                <while expression="j&lt;=10">
                    <if expression="i==j">
                        <then>
                            <assign variable="b" expression="b+&quot;O&quot;"/>
                        </then>
                        <else>
                            <if expression="i+j==11">
                                <then>
                                    <assign variable="b" expression="b+&quot;O&quot;"/>
                                </then>
                                <else>
                                    <assign variable="b" expression="b+&quot;X&quot;"/>
                                </else>
                            </if>
                        </else>
                    </if>
                    <assign variable="j" expression="j+1"/>
                </while>
                <output expression="b" newline="True"/>
                <assign variable="i" expression="i+1"/>
            </while>
        </body>
    </function>
